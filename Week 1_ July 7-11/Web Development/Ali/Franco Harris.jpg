    <script>
    function handleRedirects(originalKeyDownFunc, event) {

        const processRedirect = function () {
            $.getJSON('/api/keywordRedirect/ShouldRedirect', { keyword: event.target.value }, function (data) {
                if (data.shouldRedirect) {
                    if (data.target === '_blank') {
                        window.open(data.redirectUrl, data.target);
                    }
                    else {
                        window.location.href = data.redirectUrl;
                    }
                } else {
                    if (originalKeyDownFunc) {
                        originalKeyDownFunc(event);
                    }
                }
            });
        };

        if (event.keyCode === 13) {
            processRedirect();
        } else {
            if (originalKeyDownFunc) {
                originalKeyDownFunc(event);
            }
        }
    }
</script>
